<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault continueOnError="false" enabled="true" name="RF-raiseFault404">
  <DisplayName>RF-raiseFault404</DisplayName>
  <Properties/>
  <FaultResponse>
    <Set>
      <StatusCode>404</StatusCode>
      <ReasonPhrase>The requested resource {proxy.pathsuffix} was not found.</ReasonPhrase>
      <Payload contentType="application/json">
                {
                    "error":"404.01.001",
                    "msg": "The requested resource {proxy.pathsuffix} was not found.",
                    "link": "https://developer.example.com/404.01.001"
                }
            </Payload>
    </Set>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
