<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables name="ExtractVariables-1">
  <DisplayName>Extract ID from the URL path</DisplayName>
  <Source>request</Source>
  <URIPath>
    <Pattern ignoreCase="true">/airports/{id}</Pattern>
  </URIPath>
  <VariablePrefix>request_airport_id</VariablePrefix>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</ExtractVariables>
